class Solution {
    public int[] twoSum(int[] nums, int target) {
        
        // initialize targetArray
        int [] targetArray = new int [2];
        
    
        for(int i = 0; i < nums.length; i++){
            for(int j = nums.length-1; j > i; j--){
                if(nums[i]+nums[j] == target){
                    targetArray[0]=i;
                    targetArray[1]=j;
                    break;
                }
            }
        }   
        return targetArray;
    }
}
